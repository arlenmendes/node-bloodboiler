{
  "name": "node-bloodboiler",
  "version": "v0.1.1",
  "description": "A API built on top of expressJS",
  "main": "app.js",
  "author": "Lucas A Pelegrino <lucas.assuncao.p@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=production node app.js",
    "start:staging": "NODE_ENV=staging node app.js",
    "start:dev": "NODE_ENV=development nodemon app.js",
    "start:debug": "NODE_ENV=development node --inspect-brk=5858 app.js",
    "test": "jest --runInBand --forceExit",
    "test:coverage": "jest --runInBand --coverage --coverageReporters=text-lcov | coveralls --forceExit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "npm test"
    }
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "lodash": "^4.17.15",
    "mongoose": "^5.9.9",
    "swagger-ui-express": "^4.1.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "coveralls": "^3.0.11",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-prettier": "^3.1.3",
    "faker": "^4.1.0",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "supertest": "^4.0.2"
  }
}
